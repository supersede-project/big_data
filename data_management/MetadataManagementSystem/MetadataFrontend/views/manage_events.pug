extends layout
block headers
    link(rel='stylesheet', href='/stylesheets/loading.css')
    script(src='/js/client_manage_events.js')
    link(rel='stylesheet', href='/stylesheets/manage_styles.css')


block content
    .container
        .banner
            .row
                .col-lg-8.col-md-7.col-sm-6
                    h2 Manage Events
                    br
            .container-fluid
                .bs-component
                    table.table.table-striped.table-hover#events
                        thead
                            tr
                                th #
                                th Event
                                th Kafka Topic
                                th Dispatch To
                                th Platform
                                th Details
                                th Event Ontology
                                th Delete
                        tbody